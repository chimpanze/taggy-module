{
  "name": "@chimpanze/taggy-nuxt",
  "version": "1.1.0",
  "description": "Nuxt 3 module for the Taggy SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chimpanze/taggy-module.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && semantic-release",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@chimpanze/taggy-sdk": "^0.1.0",
    "@nuxt/kit": "^4.0.1",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint": "1.7.1",
    "@nuxt/eslint-config": "^1.7.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.1",
    "@nuxt/test-utils": "^3.19.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.1",
    "semantic-release": "^24.2.7",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "publishConfig": {
    "@chimpanze:registry": "https://npm.pkg.github.com"
  }
}
